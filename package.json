{
  "name": "store-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist/server.js",
    "devStart": "nodemon server.ts",
    "dev": "docker-compose up",
    "dev:down": "docker-compose down",
    "dev:build": "docker-compose up --build",
    "build": "tsc -p .",
    "migrate:test": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "seed:test": "dotenv -e .env.test -- npx prisma db seed",
    "generate:test": "dotenv -e .env.test -- npx prisma generate",
    "test:up": "docker-compose -f docker-compose.test.yml up -d",
    "test:down": "docker-compose -f docker-compose.test.yml down",
    "test": "npm run test:up && npm run migrate:test && dotenv -e .env.test jest -- --watchAll",
    "test:circle": "jest -i"
  },
  "prisma": {
    "seed": "tsx db/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^5.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "jsonwebtoken": "^9.0.2",
    "node": "^22.11.0",
    "nodemailer": "^6.9.16"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.9.0",
    "@types/nodemailer": "^6.4.16",
    "@types/supertest": "^6.0.2",
    "dotenv-cli": "^7.4.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-mock-extended": "^4.0.0-beta1",
    "jest-sorted": "^1.0.15",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.7",
    "prisma": "^5.22.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3"
  }
}
